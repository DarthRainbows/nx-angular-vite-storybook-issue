{
  "name": "foo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/foo/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/foo/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/foo/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/foo/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../coverage/libs/foo"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "storybook": {
      "executor": "@analogjs/storybook-angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/foo/.storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@analogjs/storybook-angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/foo",
        "configDir": "libs/foo/.storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "static-storybook": {
      "executor": "@nx/web:file-server",
      "dependsOn": ["build-storybook"],
      "options": {
        "buildTarget": "foo:build-storybook",
        "staticFilePath": "dist/storybook/foo",
        "spa": true
      },
      "configurations": {
        "ci": {
          "buildTarget": "foo:build-storybook:ci"
        }
      }
    }
  }
}
